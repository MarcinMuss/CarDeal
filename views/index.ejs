<h2 class="page-header">Search Car</h2>
<form action="/" method="GET">
  <div class="form-row">
    <div class="form-item">
      <label>Title</label>
      <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>
  </div>
  <div class="form-row">
    <div class="form-item">
      <label>Price less</label>
      <input type="number" name="priceless" value="<%= searchOptions.priceless %>">
    </div>
    <div class="form-item">
      <label>Mileage less</label>
      <input type="number" name="mileageless" value="<%= searchOptions.mileageless %>">
    </div>
  </div>
  <div class="form-row form-row-end">
    <button class="btn btn-primary" type="submit">Search</button>
  </div>
</form>
<br>
<%- include('partials/carGrid', { cars: cars, large: true }) %>